<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="deptTree" extends="common-default">
		<action name="departmentTree_*" class="cn.com.trueway.workflow.set.action.DeptTreeAction" method="{1}">
			<result name="showDepartmentTree">/WEB-INF/pages/Department/showDepartmentTree.jsp</result>
			<result name="showDepartmentTreeView">/WEB-INF/pages/Department/showDepartmentTreeView.jsp</result>
			<result name="showDepartmentTreeView_allow">/WEB-INF/pages/workflow/allow/showDepartmentTreeView.jsp</result>
			<result name="toDepartmentTree">/WEB-INF/pages/Department/toDepartmentTree.jsp</result>
			<result name="login">/WEB-INF/pages/index.jsp</result>
			<result name="fail">/WEB-INF/pages/login.jsp</result>
			<result name="error">/error.html</result>
			<result name="fail2">/fail.html</result>
			<result name="login4DA">/WEB-INF/pages/model/index.jsp</result>
			<result name="welcome">/WEB-INF/pages/welcome.jsp</result>
		</action>
	</package>
</struts>
