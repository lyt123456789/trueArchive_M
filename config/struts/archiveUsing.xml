			<result name="transferFormDetail">/WEB-INF/pages/archives/using/transferFormDetail.jsp</result>
		</action>
		
		<action name="dataUsing_*" class="cn.com.trueway.archives.using.action.DataUsingAction" method="{1}">
		    <result name="toSearchJsp">/WEB-INF/pages/archives/using/searchJsp.jsp</result>
