<struts>
	<package name="archives" extends="common-default">
		<action name="using_*" class="cn.com.trueway.archives.using.action.ArchiveUsingAction" method="{1}">
		    <result name="toshowNode">/WEB-INF/pages/archives/using/showNode.jsp</result>
		    <result name="toshowBasicdata">/WEB-INF/pages/archives/using/showBasicdata.jsp</result>
		    <result name="addBasicData">/WEB-INF/pages/archives/using/addBasicData.jsp</result>
