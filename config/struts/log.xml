<struts>
	<package name="logs" extends="common-default">
		<action name="log_*" class="logAction" method="{1}">
			<result name="logList">/WEB-INF/pages/workflow/log/logList.jsp</result>
			<result name="view">/WEB-INF/pages/workflow/log/view.jsp</result>
		</action>
	</package>
</struts>

