<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans 
							http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	 
	<bean id="allowService" class="cn.com.trueway.workflow.set.service.impl.AllowServiceImpl">
		<property name="allowDao" ref="allowDao"></property>
	</bean> 
	
	<bean id="serverPluginService" class="cn.com.trueway.workflow.set.service.impl.ServerPluginServiceImpl">
		<property name="serverPluginDao" ref="serverPluginDao"></property>
	</bean>
</beans>